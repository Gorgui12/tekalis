{
  "name": "tekalis",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:client": "npm run dev --workspace=apps/client",
    "dev:admin": "npm run dev --workspace=apps/admin",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:admin\"",
    "build:client": "npm run build --workspace=apps/client",
    "build:admin": "npm run build --workspace=apps/admin",
    "build": "npm run build:client && npm run build:admin"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "lucide-react": "^0.575.0"
  }
}
